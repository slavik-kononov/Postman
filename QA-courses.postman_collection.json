						"exec": [
							"//Проверка на статус код ответа 200\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"//Проверка на схема JSON респонса\r",
							"const schema = {\r",
							"    \"type\":\"object\",\r",
							"    \"parameters\":{\r",
							"    \"qa_salary_after_12_months\":{\"type\":\"number\"},\r",
							"    \"qa_salary_after_6_months\":{\"type\":\"number\"},\r",
							"    \"start_qa_salary\":{\"type\":\"number\"},\r",
							"    \"person\": {\r",
							"       \"type\":\"number\",\r",
							"       \"parameters\":{\r",
							"                 \"u_age\":{\"type\":\"number\"},\r",
							"                 \"u_salary_1_5_year\":{\"type\":\"number\"},\r",
							"                 \"u_name\":\r",
							"                    {\r",
							"                       \"type\":\"array\",\r",
							"                        \"items\":\r",
							"                         [\r",
							"                          {\"type\":\"integer\"},\r",
							"                          {\"type\":\"number\"},\r",
							"                          {\"type\":\"number\"}\r",
							"                         ]\r",
							"                    }\r",
							"                    \r",
							"\r",
							"}}}};\r",
							"pm.test(\"Your Response is correct by JSON validate\",function(){\r",
							"    pm.response.to.have.jsonSchema(schema);\r",
							"});\r",
							"\r",
							"//Вторая версия проверки на схему JSON ответа\r",
							"pm.test(\"Correct JSON №2\", function()\r",
							"{\r",
							"const jsonData = pm.response.json();\r",
							"var req = request.data; \r",
							"var sal = req.salary;\r",
							"pm.expect(jsonData.person).to.be.an(\"object\");\r",
							"pm.expect(jsonData.person.u_age).to.be.a(\"number\");\r",
							"pm.expect(jsonData.person.u_salary_1_5_year).to.be.a(\"number\");\r",
							"pm.expect(jsonData.person.u_name).to.be.an(\"array\")\r",
							"pm.expect(jsonData.person.u_name[0]).to.be.a(\"string\");\r",
							"pm.expect(jsonData.person.u_name[1]).to.be.a(\"number\");\r",
							"pm.expect(jsonData.person.u_name[2]).to.be.a(\"number\");\r",
							"pm.expect(jsonData.qa_salary_after_12_months).to.be.a(\"number\");\r",
							"pm.expect(jsonData.qa_salary_after_6_months).to.be.a(\"number\");\r",
							"pm.expect(jsonData.start_qa_salary).to.be.a(\"number\");\r",
							"pm.expect(jsonData.start_qa_salary).to.eq(Number(sal))\r",
							"});\r",
							"\r",
							"//Проверка правильности результата перемножения на коэффициент\r",
							"pm.test(\"You have all correct salaries \", function () {\r",
							"    const jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.qa_salary_after_6_months).to.eql(jsonData.person.u_name[1]*2);\r",
							"    pm.expect(jsonData.qa_salary_after_12_months).to.eql(jsonData.person.u_name[1]*2.9);\r",
							"    pm.expect(jsonData.person.u_salary_1_5_year).to.equal(jsonData.person.u_name[1]*4);\r",
							"});\r",
							"\r",
							" const jsonData = pm.response.json();\r",
							"console.log(\r",
							"     jsonData.person.u_name[1]*2\r",
							"           )\r",
							"     ;\r",
